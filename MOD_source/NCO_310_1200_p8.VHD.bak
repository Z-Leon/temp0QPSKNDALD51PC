					cos_p5_reg(0) <= cos_p5_reg(14);
					cos_p6_reg(0) <= cos_p6_reg(14);
					cos_p7_reg(0) <= cos_p7_reg(14);
			end if;
		end if;
	end process;

	--output
	sin_p0 <= sin_p0_reg(14);
	sin_p1 <= sin_p1_reg(14);
	sin_p2 <= sin_p2_reg(14);
	sin_p3 <= sin_p3_reg(14);
